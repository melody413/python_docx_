p_CT_TLAnimVariantIntegerVal = attribute val { xsd:int }
p_CT_TLAnimVariantFloatVal = attribute val { xsd:float }
p_CT_TLAnimVariantStringVal = attribute val { xsd:string }
p_CT_TLAnimVariant =
